{
  "name": "svelte-app",
  "type": "module",
  "description": "Figma plugin",
  "engines": {
    "node": ">=18.0.0"
  },
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --ext .ts,.js,.svelte && npx prettier --check \"src/**/*.{js,ts,svelte,html,css}\"",
    "prettier": "npx prettier --write \"src/**/*.{js,ts,svelte,html,css}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@figma/plugin-typings": "^1.51.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/node": "^22.13.4",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^2.46.0",
    "figma-ui3-kit-svelte": "^0.3.2",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "prettier": "3.7.3",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^4.2.20",
    "svelte-eslint-parser": "^0.39.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.2.0",
    "vitest": "^4.0.14"
  },
  "dependencies": {}
}
